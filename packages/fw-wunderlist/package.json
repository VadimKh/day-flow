{
  "name": "@cli-assistant/fw-wunderlist",
  "description": "Working with wunderlist",
  "version": "1.0.1",
  "author": "VadimKh @VadimKh",
  "bugs": "https://github.com/VadimKh/fw-wunderlist/issues",
  "dependencies": {
    "@cli-assistant/fw-core": "^1.0.1",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "axios": "^0.21.1",
    "configstore": "^4.0.0",
    "enquirer": "^2.0.6",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/VadimKh/fw-wunderlist",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init_app": "./lib/hooks/init/initApp",
      "fw_today": "./lib/hooks/today/fwToday"
    }
  },
  "repository": "VadimKh/fw-wunderlist",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
